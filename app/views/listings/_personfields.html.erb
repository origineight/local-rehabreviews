<div class="form-group">
  <%= label_tag 'listing_title', 'Title', class: 'control-label col-sm-2' %>
  <div class="col-sm-10">
    <%= text_field_tag 'listing[title]', @listing.title, class: 'form-control', id: 'listing_title' %>
  </div>
</div>
<div class="form-group required">
  <%= label_tag 'listing_first_name', 'First Name', class: 'control-label col-sm-2' %>
  <div class="col-sm-10">
    <%= text_field_tag 'listing[first_name]', @listing.first_name, :required => true, class: 'form-control', id: 'listing_first_name' %>
  </div>
</div>
<div class="form-group">
  <%= label_tag 'listing_middle_name', 'Middle Name', class: 'control-label col-sm-2' %>
  <div class="col-sm-10">
    <%= text_field_tag 'listing[middle_name]', @listing.middle_name, class: 'form-control', id: 'listing_middle_name' %>
  </div>
</div>
<div class="form-group required">
  <%= label_tag 'listing_last_name', 'Last Name', class: 'control-label col-sm-2' %>
  <div class="col-sm-10">
    <%= text_field_tag 'listing[last_name]', @listing.last_name, :required => true, class: 'form-control', id: 'listing_last_name' %>
  </div>
</div>
<div class="form-group">
  <%= label_tag 'listing_suffix', 'Suffix', class: 'control-label col-sm-2' %>
  <div class="col-sm-10">
    <%= text_field_tag 'listing[suffix]', @listing.suffix, class: 'form-control', id: 'listing_suffix' %>
  </div>
</div>
